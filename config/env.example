# ContaPYME - Variables de Entorno
# Copia este archivo como .env y configura tus valores

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_APP_NAME=ContaPYME
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# API Endpoints (opcional)
VITE_MERCADO_PAGO_PUBLIC_KEY=your_mercado_pago_public_key
VITE_GOOGLE_DRIVE_CLIENT_ID=your_google_drive_client_id
VITE_WHATSAPP_API_TOKEN=your_whatsapp_api_token

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info 